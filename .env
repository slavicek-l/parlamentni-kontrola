# App
ENV=dev
PUBLIC_PORT=8080
CORS_ORIGINS=http://localhost:8080,http://localhost:5173
API_RATE_LIMIT_PER_MIN=120
API_AUTH_MODE=none # none|apikey|jwt
API_KEYS=devkey1,devkey2
JWT_SECRET=devsecret
JWT_ALG=HS256
JWT_ACCESS_TTL=3600

# Backend
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000
LOG_LEVEL=INFO

# Database
POSTGRES_USER=app
POSTGRES_PASSWORD=app
POSTGRES_DB=pkontrola
POSTGRES_HOST=db
POSTGRES_PORT=5432

# Redis
REDIS_URL=redis://redis:6379/0

# Celery
CELERY_BROKER_URL=redis://redis:6379/1
CELERY_RESULT_BACKEND=redis://redis:6379/2
CELERY_BEAT_ENABLED=true

# ETL
PSP_DATA_BASE=https://www.psp.cz/eknih/cdrom/opendata
ETL_DOWNLOAD_DIR=/data/downloads
ETL_WORK_DIR=/data/work
ETL_USER_AGENT=ParlamentniKontrola/1.0 (+https://parlamentnikontrola.cz)
ETL_AUDIT_ALERT_THRESHOLD=0.02

# Frontend
VITE_API_BASE=http://localhost:8080/api/v1
